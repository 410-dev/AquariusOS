{
    "StructType": "BuildStructure",
    "StructVersion": 1,
    "Name": "AquariusOS Default Build Structure",
    "Version": "1.0.0",
    "Source": "src",
    "Temporary": "tmp",
    "Output": "build",
    "TargetDistro": "Ubuntu",
    "PreprocessorConfig": {
        "Variables": {
            "BUILD_DATE": "RUN:date +%y%m%d"
        },
        "PathReplacements": {
            "aisp-": "aqua-",
            "com.acadia.": "me.hysong.",
            "aisp": "aqua",
            "AISP": "AquariusOS"
        },
        "BlacklistedFiles": [
            ".DS_Store",
            "Thumbs.db"
        ],
        "SetExecutables": [
            "*.sh",
            "preinst",
            "postinst",
            "prerm",
            "postrm"
        ]
    },
    "Patches": {
        "rocky": false
    },
    "Mapping": {
        "package-meta/ubuntu": "/DEBIAN",
        "applications/system": "/opt/aqua/sys/applications",
        "applications/user": "/opt/aqua/applications",
        "core": "/opt/aqua/sys",
        "features": "/opt/aqua/features",
        "libraries/system": "/opt/aqua/sys/libs",
        "libraries/user": "/opt/aqua/libs",
        "services/system": "/opt/aqua/sys/services",
        "services/user": "/opt/aqua/services"
    },
    "Packaging": {
        "CommandLines": [
            ["sudo", "chown", "-R", "root:root", "$1"],
            ["sudo", "chmod", "-R", "755", "$1"],
            ["sudo", "dpkg-deb", "--build", "$1"]
        ],
        "OutputPatterns": [
            "*.deb"
        ]
    }
}